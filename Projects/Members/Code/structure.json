    "query": {
        "from": {
            "table": "Projects\\Projects\\Members\\Code\\Tables\\Members",
            "alias": "ppm",
            "name": "#__projects_projects_members"
        },
        "select": [
            "ppm.id",
            "ppm.project_id",
            "pp.title AS project_id_title",
            "ppm.user_id",
            "uu.name AS user_id_name",
            "ppm.is_sent",
            "ppm.created_by",
            "ppm.date_created",
            "ppm.modified_by",
            "ppm.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Projects\\Projects\\Code\\Tables\\Projects",
                "alias": "pp",
                "name": "#__projects_projects",
                "on": "ppm.project_id = pp.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "ppm.user_id = uu.id"
            }
        ]
    }
